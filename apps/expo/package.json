{
  "name": "expo-simple-commerce",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "scripts": {
    "compile": "echo skip",
    "start": "expo start --dev-client",
    "dev": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "yarn start --web",
    "eject": "expo eject",
    "publish": "npx cross-env ENV=production expo publish",
    "test": "jest",
    "test:all": "yarn test --coverage",
    "test:e2e": "echo skipping",
    "test:ci": "cross-env CI=true jest --watchAll=false --runInBand",
    "check-types": "tsc",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore --ext .js,jsx,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json|css|scss|html)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "CI=true npm-run-all --parallel test check-types check-format lint",
    "build:ios": "eas build --profile dev --platform ios",
    "build:android": "eas build --profile dev --platform android",
    "build:all": "eas build --profile dev --platform all",
    "deploy:ios": "eas build --profile preview --auto-submit-with-profile=development --platform ios",
    "deploy:android": "eas build --profile preview --platform android",
    "deploy:android:preview": "eas build --profile preview:production --platform android",
    "deploy:android:production": "eas build --profile production --platform android",
    "deploy:ios:production": "eas build --profile production --auto-submit-with-profile=production --platform ios"
  },
  "dependencies": {
    "@expo/metro-config": "~0.17.1",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/netinfo": "9.3.7",
    "@react-native-firebase/analytics": "^16.5.0",
    "@react-native-firebase/app": "^16.5.0",
    "@react-native-firebase/auth": "^16.5.0",
    "@react-native-firebase/dynamic-links": "^16.5.0",
    "@react-native-firebase/firestore": "^16.5.0",
    "@react-native-firebase/messaging": "^16.5.0",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/elements": "^1.3.12",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.10",
    "@react-spring/native": "^9.6.1",
    "@segment/analytics-react-native": "^2.10.0",
    "@segment/sovran-react-native": "^0.4.3",
    "@simple/services": "*",
    "@simple/stores": "*",
    "@simple/utils": "*",
    "artisn-rn": "^0.1.0-canary.118",
    "expo": "~50.0.19",
    "expo-apple-authentication": "~6.3.0",
    "expo-build-properties": "~0.11.1",
    "expo-constants": "~15.4.6",
    "expo-dev-client": "~3.3.12",
    "expo-file-system": "~16.0.9",
    "expo-linking": "~6.2.2",
    "expo-localization": "~14.8.4",
    "expo-location": "~16.5.5",
    "expo-screen-orientation": "~6.4.1",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.4",
    "i18n-js": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.14",
    "react-native-error-boundary": "^1.1.17",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-fbsdk-next": "^12.1.4",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.9"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@simple/builders": "*",
    "@simple/types": "*",
    "@types/i18n-js": "^3.8.4",
    "@types/react-native-snap-carousel": "^3.8.5",
    "jest-expo": "~50.0.4",
    "react-native-svg-transformer": "^1.0.0"
  }
}
